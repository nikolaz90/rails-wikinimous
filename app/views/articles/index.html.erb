<section>
  <h1>Welcome to the wiki</h1>
  <ul class="articles-container-list">
    <% @articles.each do |item| %>
    <article>
      <div>
        <p><strong><%= link_to item.title, article_path(item) %></strong></p>
        <p><%= item.content %></p>
      </div>
      <%= link_to 'edit', edit_article_path(item)%>
      <%= link_to 'delete', article_path(item), data: {turbo_method: :delete}%>
    </article>

    <% end %>
  </ul>
</section>
